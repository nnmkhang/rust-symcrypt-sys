ARM64X_EC_ENABLED=1

TARGETNAME=SymCryptKernelTestModule_UM
TARGETTYPE=DYNLINK

# Use __declspec(dllexport) for this module to make it easier to keep dll and driver in sync
DLLDEF=

MSC_WARNING_LEVEL=/W4 /WX

C_DEFINES=$(C_DEFINES) -DINCLUDE_IMPL_MSBIGNUM=0 -DINCLUDE_IMPL_RSA32=0

INCLUDES= \
    ..\inc; \
    ..\..\inc ; \
    $(DDK_INC_PATH); \

SOURCES= \
    module.cpp \

TARGETLIBS= \
    $(MINWIN_EXTERNAL_SDK_LIB_PATH_L)\ntdll.lib \
    $(MINCORE_EXTERNAL_SDK_LIB_VPATH_L)\mincore.lib \
    $(ONECORE_EXTERNAL_SDK_LIB_PATH)\bcrypt.lib \
    $(OBJECT_ROOT)\symcrypt\lib\$(O)\symcrypt.lib \

DLLDEF_OBJECTS= \
    $(OBJECT_ROOT)\symcrypt\lib\$(O)\symcrypt.lib \

UNICODE = 1

USE_MSVCRT=1
USE_UNICRT=1

BUFFER_OVERFLOW_CHECKS=1

ENABLE_ISO_VOLATILE_WARNING=1

PRECOMPILED_INCLUDE = precomp.h
PRECOMPILED_CXX = 1
USE_RTTI = 1

USE_STL = 1
STL_VER = STL_VER_CURRENT
USE_NATIVE_EH = 1

_NT_TARGET_VERSION=$(_NT_TARGET_VERSION_WINTHRESHOLD)
